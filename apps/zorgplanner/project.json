{
  "name": "zorgplanner",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "prefix": "zorgplanner",
  "sourceRoot": "apps/zorgplanner/src",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@angular-devkit/build-angular:browser",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/apps/zorgplanner",
        "index": "apps/zorgplanner/src/index.html",
        "main": "apps/zorgplanner/src/main.ts",
        "polyfills": ["zone.js"],
        "tsConfig": "apps/zorgplanner/tsconfig.app.json",
        "inlineStyleLanguage": "scss",
        "assets": [
          "apps/zorgplanner/src/favicon.ico",
          "apps/zorgplanner/src/assets",
          "apps/zorgplanner/src/.htaccess"
        ],
        "styles": ["apps/zorgplanner/src/styles.scss"],
        "scripts": []
      },
      "configurations": {
        "production": {
          "aot": true,
          "buildOptimizer": true,
          "budgets": [
            {
              "type": "initial",
              "maximumWarning": "500kb",
              "maximumError": "1mb"
            },
            {
              "type": "anyComponentStyle",
              "maximumWarning": "2kb",
              "maximumError": "4kb"
            }
          ],
          "outputHashing": "all"
        },
        "development": {
          "buildOptimizer": false,
          "optimization": false,
          "vendorChunk": true,
          "extractLicenses": false,
          "sourceMap": true,
          "namedChunks": true,
          "fileReplacements": [
            {
              "replace": "apps/zorgplanner/src/environments/environment.ts",
              "with": "apps/zorgplanner/src/environments/environment.development.ts"
            }
          ]
        },
        "local": {
          "buildOptimizer": false,
          "optimization": false,
          "vendorChunk": true,
          "extractLicenses": false,
          "sourceMap": true,
          "namedChunks": true,
          "fileReplacements": [
            {
              "replace": "apps/zorgplanner/src/environments/environment.ts",
              "with": "apps/zorgplanner/src/environments/environment.local.ts"
            }
          ]
        }
      },
      "defaultConfiguration": "production"
    },
    "serve": {
      "executor": "@angular-devkit/build-angular:dev-server",
      "configurations": {
        "production": {
          "buildTarget": "zorgplanner:build:production"
        },
        "development": {
          "buildTarget": "zorgplanner:build:development"
        },
        "local": {
          "buildTarget": "zorgplanner:build:local"
        }
      },
      "defaultConfiguration": "local"
    },
    "extract-i18n": {
      "executor": "@angular-devkit/build-angular:extract-i18n",
      "options": {
        "buildTarget": "zorgplanner:build"
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": [
          "apps/zorgplanner/**/*.ts",
          "apps/zorgplanner/**/*.html"
        ]
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "apps/zorgplanner/jest.config.ts"
      }
    },
    "serve-static": {
      "executor": "@nx/web:file-server",
      "options": {
        "buildTarget": "zorgplanner:build"
      }
    }
  }
}
